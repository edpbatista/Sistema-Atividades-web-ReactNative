// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Atividade {
  id   Int    @id @default(autoincrement())
  nome String
  data String

  @@map("atividades")
}

model Usuario {
  id      Int    @id @default(autoincrement())
  nomeDeUsuario  String  @unique
  senha   String
  nome    String

  @@map("usuario")
}
